<resources>
    <!-- Application theme. -->
    <style name="DarkTheme" parent="MaterialDrawerTheme">
        <item name="windowActionModeOverlay">true</item>
        <!--
        All customizations that are NOT specific to a particular API-level can go here.
                <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@null</item>
        -->
    </style>


    <style name="LightTheme" parent="MaterialDrawerTheme.Light">
        <item name="windowActionModeOverlay">true</item>
        <!--
       android:textColorPrimaryInverse is the  color of the title
       text in the Toolbar, in the Theme.AppCompat.Light theme:
        -->
        <!-- <item name="android:textColorPrimaryInverse">@color/abc_primary_text_material_light</item> -->'
    </style>

    <style name="BlueTheme" parent="DarkTheme">
        <!-- Main theme colors -->
        <!-- your app branding color for the app bar -->
        <item name="colorPrimary">@color/material_blue_primary</item>
        <!-- darker variant for the status bar and contextual app bars -->
        <item name="colorPrimaryDark">@color/material_blue_dark</item>
        <!-- theme UI controls like checkboxes and text fields -->
        <item name="colorAccent">@color/material_blue_accent</item>
    </style>

